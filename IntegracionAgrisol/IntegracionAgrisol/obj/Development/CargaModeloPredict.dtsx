                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT       proD.proveedorID,plaD.plantaID, tempD.temporadaID,cliD.productoID,tiD.tiempoID,detTR.kilos_comprado,detTR.porcentaje_castigo,detTR.precio_ofrecido,CASE  WHEN detTR.porcentaje_castigo &gt;= 8 THEN 'MALO' WHEN detTR.porcentaje_castigo &lt;=7 AND detTR.porcentaje_castigo &gt;= 4 THEN 'NORMAL'WHEN detTR.porcentaje_castigo &lt;= 3 THEN 'BUENO'  END AS estado
																																																																																																																																																																																																													
FROM            AgrisolTR.dbo.Detalle_Compra detTR INNER JOIN
                         AgrisolTR.dbo.Compra ON detTR.detalleID = AgrisolTR.dbo.Compra.compraID INNER JOIN
                         AgrisolTR.dbo.Producto ON detTR.productoID = AgrisolTR.dbo.Producto.productoID INNER JOIN
                         AgrisolTR.dbo.Categoria ON AgrisolTR.dbo.Producto.categoriaID = AgrisolTR.dbo.Categoria.categoriaID INNER JOIN
                         AgrisolTR.dbo.Proveedor ON AgrisolTR.dbo.Compra.proveedorID = AgrisolTR.dbo.Proveedor.proveedorID INNER JOIN
                         AgrisolTR.dbo.Planta ON AgrisolTR.dbo.Proveedor.plantaID = AgrisolTR.dbo.Planta.plantaID INNER JOIN
                         AgrisolTR.dbo.Comuna ON AgrisolTR.dbo.Proveedor.comunaID = AgrisolTR.dbo.Comuna.comunaID INNER JOIN
                         AgrisolTR.dbo.Provincia ON AgrisolTR.dbo.Comuna.provinciaID = AgrisolTR.dbo.Provincia.provinciaID INNER JOIN
                         AgrisolTR.dbo.Region ON AgrisolTR.dbo.Provincia.regionID = AgrisolTR.dbo.Region.regionID INNER JOIN
                         AgrisolTR.dbo.Temporada ON AgrisolTR.dbo.Compra.temporadaID = AgrisolTR.dbo.Temporada.temporadaID AND AgrisolTR.dbo.Planta.plantaID = AgrisolTR.dbo.Temporada.plantaID INNER JOIN
                         AgrisolTR.dbo.Tipo_Pago ON AgrisolTR.dbo.Compra.tipopagoID = AgrisolTR.dbo.Tipo_Pago.tipopagoID INNER JOIN
						 AgrisolDM.dbo.DT_Proveedor proD ON AgrisolTR.dbo.Proveedor.proveedorID = proD.proveedorID INNER JOIN
						 AgrisolDM.dbo.DT_Planta plaD  ON AgrisolTR.dbo.Planta.nombre_planta = plaD.nombre_planta INNER JOIN
						 AgrisolDM.dbo.DT_Temporada tempD ON AgrisolTR.dbo.Temporada.nombre_temporada = tempD.nombre_temporada INNER JOIN
						 AgrisolDM.dbo.DT_Producto cliD ON AgrisolTR.dbo.Producto.nombre_producto = cliD.nombre_producto INNER JOIN
						 AgrisolDM.dbo.DT_Tiempo  tiD ON  tiD.fecha = CONVERT(DATE,AgrisolTR.dbo.Compra.fecha_compra)</property>
